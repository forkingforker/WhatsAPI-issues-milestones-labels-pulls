            "body": "    public function SendAddParticipants($groupJID, $participants)\r\n    {\r\n\t$id = \"add_group_participants_\" . time() . '-' . $this->_msgCounter;\r\n\t$this->_msgCounter++;\r\n\t$this->SendVerbParticipants($groupJID, $participants, $id, \"add\");\r\n    }\r\n\r\n    protected function SendVerbParticipants($groupJID, $participants, $id, $innerTag)\r\n    {\r\n\t$_participants = array();\r\n\tforeach($participants as $participant)\r\n\t{\r\n\t\t$_participants[] = new ProtocolNode(\"participant\", array(\"jid\" => $participant), null, \"\");\r\n\t}\r\n\t\r\n\t$hash = array();\r\n\t$hash[\"xmlns\"] = \"w:g\";\r\n\t\r\n\t$child = new ProtocolNode($innerTag, $hash, $_participants, \"\");\r\n\t\r\n\t$hash = array();\r\n\t$hash[\"id\"] = $id;\r\n\t$hash[\"type\"] = \"set\";\r\n\t$hash[\"to\"] = $groupJID;\r\n\t\r\n\t$node = new ProtocolNode(\"iq\", $hash, $child, \"\");\r\n\t\r\n\t$this->sendNode($node);\r\n    }",
